	@$(CHK_DIR_EXISTS) .tmp/webServer1.0.0 || $(MKDIR) .tmp/webServer1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/webServer1.0.0/ && $(COPY_FILE) --parents log.h net.h .tmp/webServer1.0.0/ && $(COPY_FILE) --parents main.cpp log.cpp net.cpp .tmp/webServer1.0.0/ && (cd `dirname .tmp/webServer1.0.0` && $(TAR) webServer1.0.0.tar webServer1.0.0 && $(COMPRESS) webServer1.0.0.tar) && $(MOVE) `dirname .tmp/webServer1.0.0`/webServer1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/webServer1.0.0


