####### Compiler, tools and options

CC            = gcc
CXX           = g++
DEFINES       = -DQT_NO_DEBUG
CFLAGS        = -Wall -W -D_REENTRANT $(DEFINES)
CXXFLAGS      = -Wall -W -D_REENTRANT $(DEFINES)
LINK          = g++
LFLAGS        = 
LIBS          = -L/usr/lib/x86_64-linux-gnu -lpthread 
DEL_FILE      = rm -f

####### Output directory

OBJECTS_DIR   = ./

####### Files

SOURCES       = main.cpp \
		log.cpp \
		net.cpp 
OBJECTS       = main.o \
		log.o \
		net.o
DESTDIR       = 
TARGET        = webServer

first: all
####### Implicit rules

.SUFFIXES: .o .c .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o "$@" "$<"

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o "$@" "$<"

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

clean: 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) $(TARGET)
	-$(DEL_FILE) *~ core *.core

####### Compile

main.o: main.cpp log.h \
		net.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

log.o: log.cpp log.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o log.o log.cpp

net.o: net.cpp net.h \
		log.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o net.o net.cpp
